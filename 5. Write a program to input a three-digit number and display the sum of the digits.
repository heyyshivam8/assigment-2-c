//5. Write a program to input a three-digit number and display the sum of the digits.
#include <stdio.h>

int main()
{
    int temp,sum=0,num;
    printf("Enter the three digit number:");
    scanf("%d",&num);
    while(num>0)
    {
        temp=num%10;
        sum=sum+temp;
        num=num/10;
    }
    printf("%d is the sum of three digit no.",sum);
    return 0;
}
